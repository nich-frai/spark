{
  "name": "spark-framework",
  "version": "0.0.1",
  "description": "",
  "main": "./dist/main.js",
  "type": "module",
  "scripts": {
    "dev": "node .scripts/dev.mjs",
    "generate": "prisma generate",
    "migrate": "prisma migrate dev",
    "test": "vitest --config ./vitest.config.ts",
    "coverage": "vitest --config ./vitest.config.ts run coverage"
  },
  "engines": {
    "node": ">=14.16"
  },
  "imports": {
    "#http/*": "./dist/http/*.js",
    "#ws/*": "./dist/websocket/*.js",
    "#utils/*": "./dist/utils/*.js",
    "#config/*": "./dist/config/*.js",
    "#models/*": "./dist/app/models/*.js",
    "#services/*": "./dist/app/services/*.js",
    "#common/*": "./dist/app/common/*.js",
    "#container": "./dist/dependency_injection/container.js",
    "#container/*": "./dist/dependency_injection/*.js",
    "#logger": "./dist/core/logger/logger.js"
  },
  "exports" : {
    "." : "./dist/index.js",
    "./http" : "./dist/http/index.js",
    "./logger" : "./dist/logger/logger.js",
    "./utils" : "./dist/utils/index.js",
    "./ws" : "./dist/ws/index.js",
    "./type" : "./dist/typebox/index.js"
  },
  "keywords": [
    "REST",
    "Typescript",
    "NodeJS",
    "Spark",
    "API",
    "Fast prototyping"
  ],
  "author": {
    "name": "Nicholas Frai",
    "url": "https://github.com/Nonanick"
  },
  "license": "MIT",
  "dependencies": {
    "@sinclair/typebox": "^0.29.6",
    "awilix": "^8.0.1",
    "dotenv": "^16.3.1",
    "find-my-way": "^7.6.2",
    "formidable": "^3.5.0",
    "pino": "^8.14.2"
  },
  "devDependencies": {
    "@types/formidable": "^3.4.0",
    "@types/node": "^20.4.5",
    "esbuild": "^0.18.17",
    "kleur": "^4.1.5",
    "tsx": "^3.12.7",
    "type-fest": "^4.0.0",
    "vitest": "^0.33.0"
  }
}